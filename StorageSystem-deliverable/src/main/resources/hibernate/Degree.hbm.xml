<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="no.dh.storagesystem.model.Degree" table="degree" lazy="false">

		<id name="id">
			<generator class="native" />
		</id>

		<property name="type" not-null="true" unique="true"/>
		

		<set name="requiredCourses" table="requiredCourses" >
			<key column="degree_id" />
			<many-to-many column="course_id"
				class="no.dh.storagesystem.model.Course" />
		</set>

	</class>
</hibernate-mapping>