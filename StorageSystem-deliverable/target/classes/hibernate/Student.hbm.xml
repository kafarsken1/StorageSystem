<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="no.dh.storagesystem.model.Student" table="student" lazy="false">

		<id name="id">
			<generator class="native" />
		</id>

		<property name="name" column="student_name" not-null="true" unique="true"/>
		
		<property name="latitude" column="latitude"/>
		
		<property name="longitude" column="longitude"/>


		<set name="courses" table="student_courses" inverse="true" lazy="false">
			<key column="student_id" />
			<many-to-many column="course_id"
				class="no.dh.storagesystem.model.Course" />
		</set>

		<set name="degrees" table="student_degrees" lazy="false">
			<key column="student_id" />
			<many-to-many column="degree_id"
				class="no.dh.storagesystem.model.Degree" />
		</set>
	</class>
</hibernate-mapping>
